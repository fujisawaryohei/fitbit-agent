# Spring Boot DB connection（docker/.env の POSTGRES_USER / POSTGRES_PASSWORD と同じ値を設定する）
DB_USERNAME=postgres
DB_PASSWORD=postgres

# Fitbit OAuth2.0（dev.fitbit.com で取得）
FITBIT_CLIENT_ID=your_client_id_here
FITBIT_CLIENT_SECRET=your_client_secret_here

# Token encryption key（32文字以上のランダムな文字列）
TOKEN_ENCRYPTION_KEY=dev-encryption-key-32bytes-pad!
